<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GlavniIzbornikKorisnik">


    <Spinner
        android:id="@+id/spinZupanije"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spinKategorije"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="-7dp" />

    <ImageView
        android:id="@+id/imgAdd"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_below="@+id/btnIstrazi"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="158dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="164dp"
        android:padding="3dp"
        app:srcCompat="@drawable/ic_add_circle_24dp" />

    <ImageView
        android:id="@+id/imgNotification"
        android:layout_width="78dp"
        android:layout_height="58dp"
        android:layout_below="@+id/btnIstrazi"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="248dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="85dp"
        android:padding="3dp"
        app:srcCompat="@drawable/ic_notifications_24dp" />

    <ImageView
        android:id="@+id/imgProfile"
        android:layout_width="78dp"
        android:layout_height="58dp"
        android:layout_below="@+id/btnIstrazi"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="329dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="4dp"
        android:padding="3dp"
        app:srcCompat="@drawable/ic_person_24dp" />

    <ImageView
        android:id="@+id/imgWrench"
        android:layout_width="78dp"
        android:layout_height="58dp"
        android:layout_below="@+id/btnIstrazi"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="254dp"
        android:padding="3dp"
        app:srcCompat="@drawable/ic_wrench_24dp" />

    <TextView
        android:id="@+id/txtPocetna"
        android:layout_width="match_parent"
        android:layout_height="185dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="0dp"
        android:background="#27D570"
        android:text="    Početna"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="#27D570"
        android:text=" " />

    <ImageView
        android:id="@+id/imgKorisnik"
        android:layout_width="wrap_content"
        android:layout_height="213dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="275dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:srcCompat="@drawable/korisnik" />

    <ImageView
        android:id="@+id/imgIzvodjac"
        android:layout_width="wrap_content"
        android:layout_height="218dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="145dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="135dp"
        app:srcCompat="@drawable/obrtnik" />

    <Button
        android:id="@+id/btnIstrazi"
        android:layout_width="169dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/spinZupanije"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="120dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="120dp"
        android:backgroundTint="#27D570"
        android:text="Istraži" />

    <TextView
        android:id="@+id/txtIstraziObrt"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_below="@+id/txtPocetna"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="33dp"
        android:text="Istražite izvođače"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imgHome"
        android:layout_width="78dp"
        android:layout_height="58dp"
        android:layout_below="@+id/btnIstrazi"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="333dp"
        android:padding="3dp"
        app:srcCompat="@drawable/ic_home_24dp" />

    <Spinner
        android:id="@+id/spinKategorije"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtIstraziObrt"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="-9dp" />

</RelativeLayout>